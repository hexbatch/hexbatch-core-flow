{% if applied.tagged_flow_project_guid %}
    {% if project.flow_project_guid == applied.tagged_flow_project_guid %}
        <i class="bi bi-box bg-dark text-info" ></i>
        {{ applied.tagged_title }}
    {% else %}
        <i class="bi bi-box"></i>
        {{ applied.tagged_title }}
    {% endif %}

{% elseif applied.tagged_flow_user_guid %}
    <i class="bi bi-person-circle"></i>
    {{ applied.tagged_title }}
{% endif %}