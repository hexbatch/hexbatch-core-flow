<div class="col-12 col-xxl-6 ">
    <div class="container p-0 m-1">

        <div class="card" >

            <div class="card-header p-0 d-flex justify-content-center bg-white pb-1 pt-1">
                <div class="user-meta-here"></div>
            </div>

            <div class="card-body">
                <h5 class="card-title">
                    {{ project.flow_project_blurb }}
                </h5>

                <div class="flow-project-readme-display">
                    {{ project.flow_project_readme_html| raw }}
                </div>



            </div>
        </div>
    </div>
</div><!-- /.col -->

<script>
    jQuery(function($){
        let meta_tags = will_echo_tag_array({{ meta_tags|to_json_string }});
        let home = $(`.user-meta-here`);
        for(let i = 0; i < meta_tags.length ; i++) {
            let tag = meta_tags[i];
            flow_standards_generate_view('meta',tag,home);
        }
    });



</script>