<div class="col-12 col-xxl-6 ">
    <div class="container p-0 m-1">

        <div class="card" >

            <div class="card-header">
                <h4 class="">
                     {{ dat_user.flow_user_name }}
                </h4>
            </div>

            <div class="card-body">
                <h5 class="card-title">
                    {{ project.flow_project_blurb }}
                </h5>

                <div class="flow-project-readme-display">
                    {{ project.flow_project_readme_html| raw }}
                </div>

                <div class="user-meta-here m-2"></div>

            </div>
        </div>
    </div>
</div><!-- /.col -->

<script>
    jQuery(function($){
        let tag_guids = {{ meta_tag_guids|to_json_string }};
        let home = $(`.user-meta-here`);
        for(let i = 0; i < tag_guids.length ; i++) {
            let guid = tag_guids[i];
            flow_standards_generate_view('meta',guid,home);
        }
    });



</script>